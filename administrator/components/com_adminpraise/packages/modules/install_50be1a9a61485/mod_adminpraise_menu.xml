<?xml version="1.0" encoding="utf-8"?>
<install version="1.6" type="module" client="administrator" publish="true" position="adminpraise_menu" method="upgrade">
	<name>Adminpraise Menu</name>
	<version>1.0.0-build578</version>
	<creationDate>2011-12-07</creationDate>
	<author>adminpraise.com</author>
	<authorEmail>services@compojoom.com</authorEmail>
	<authorUrl>http://adminpraise.com</authorUrl>
	<copyright>Adminpraise</copyright>
	<license>Adminpraise.com</license>
	<description><![CDATA[Adminpraise Menu is the menu module used in Adminpraise3.]]></description>
	<files>
	    <folder>dynamic</folder>
	    <folder>tmpl</folder>
	    <filename>helper.php</filename>
	    <filename>index.html</filename>
	    <filename module="mod_adminpraise_menu">mod_adminpraise_menu.php</filename>
	</files>
	<config>
		<fields name="params">
			<fieldset name="basic">
				<field
					name="moduleclass_sfx"
					type="text"
					default=""
					label="MODULE CLASS SUFFIX"
					description="PARAMMODULECLASSSUFFIX" />
				<field
					id="moduleid_css"
					name="moduleid_css"
					type="text"
					label="MENU ID CSS"
					description="The id of the top menu ul element"
					default=""
					class="inputbox" />
				<field
					name="menutype"
					type="adminpraisemenutype"
					default="1"
					label="Menu Name"
					description="The name of the menu (default is z-adminpraise-menu)" />
			</fieldset>
		</fields>
	</config>
</install>
