<?xml version="1.0" encoding="utf-8"?>
<install version="1.6" type="module" client="administrator" publish="true" position="adminpraise_search" method="upgrade">
	<name>My Editor</name>
	<version>1.1</version>
	<creationDate>2009/08/19</creationDate>
	<author>www.JoomlaPraise.com</author>
	<authorEmail>support@joomlapraise.com</authorEmail>
	<authorUrl>http://www.joomlapraise.com</authorUrl>
	<copyright>JoomlaPraise</copyright>
	<license>GNU/GPL</license>
	<description>MY_EDITOR_FOR_JOOMLA</description>
	<files>
		<filename module="mod_myeditor">mod_myeditor.php</filename>
		<filename>mod_myeditor.xml</filename>
		<filename>index.html</filename>
		<filename>helper.php</filename>
		<folder>tmpl</folder>
		<folder>assets</folder>
	</files>
	<languages folder="administrator/language/">
        <language tag="en-GB">en-GB/en-GB.mod_myeditor.ini</language>
  </languages>

	<config>
		<fields name="params">
			<fieldset name="basic">
				<field
					name="moduleclass_sfx"
					type="text"
					default=""
					label="MODULE CLASS SUFFIX"
					description="PARAMMODULECLASSSUFFIX" />
				<field
					id="mod_myeditor"
					name="mod_myeditor"
					type="cmsmarketconnect"
					label="Version Information"
					description="Up to date version, support, and review information from CMS Market."
					default="" />
			</fieldset>
		</fields>
	</config>

</install>
