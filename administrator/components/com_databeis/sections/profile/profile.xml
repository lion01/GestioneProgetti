	<files folder="data"></files>
	
	<params>
	   <param type="slider_start" title="PFL_CONTACT_SETTINGS" id="-contact"/>
	      <param type="select" name="allow_phone" title="PFL_ALLOW_PHONE" desc="PFL_ALLOW_PHONE_DESC">
	         <option value="1">PFL_YES</option>
	         <option value="0">PFL_NO</option>
	      </param>
	      <param type="select" name="allow_mphone" title="PFL_ALLOW_MPHONE" desc="PFL_ALLOW_MPHONE_DESC">
	         <option value="1">PFL_YES</option>
	         <option value="0">PFL_NO</option>
	      </param>
	      <param type="select" name="allow_skype" title="PFL_ALLOW_SKYPE" desc="PFL_ALLOW_SKYPE_DESC">
	         <option value="1">PFL_YES</option>
	         <option value="0">PFL_NO</option>
	      </param>
	      <param type="select" name="allow_msn" title="PFL_ALLOW_MSN" desc="PFL_ALLOW_MSN_DESC">
	         <option value="1">PFL_YES</option>
	         <option value="0">PFL_NO</option>
	      </param>
	      <param type="select" name="allow_icq" title="PFL_ALLOW_ICQ" desc="PFL_ALLOW_ICQ_DESC">
	         <option value="1">PFL_YES</option>
