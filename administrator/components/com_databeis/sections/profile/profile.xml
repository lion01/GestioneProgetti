	         <option value="1">PFL_YES</option>
	         <option value="0">PFL_NO</option>
	      </param>
	      <param type="select" name="allow_zip" title="PFL_ALLOW_ZIP" desc="PFL_ALLOW_ZIP_DESC">
	         <option value="1">PFL_YES</option>
	         <option value="0">PFL_NO</option>
	      </param>
	   <param type="slider_end"/>
