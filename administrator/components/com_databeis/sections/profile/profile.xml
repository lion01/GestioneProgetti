	         <option value="0">PFL_NO</option>
	      </param>
	      <param type="text" name="upload_path" title="PFL_UPLOAD_PATH" desc="PFL_UPLOAD_PATH_DESC" size="40"/>
	      <param type="text" name="max_w" title="PFL_AVATAR_MAX_WIDTH" desc="PFL_AVATAR_MAX_WIDTH_DESC" size="5"/>
	      <param type="text" name="max_h" title="PFL_AVATAR_MAX_HEIGHT" desc="PFL_AVATAR_MAX_HEIGHT_DESC" size="5"/>
